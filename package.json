{
  "name": "just-a-note",
  "version": "1.0.0",
  "description": "A blog",
  "author": "Shuji Narazaki",
  "private": true,
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "update": "processmd article/**/*.md --stdout --outputDir article/.json > article/.json/db.json",
    "now-build": "npm run update && npm run generate"
  },
  "dependencies": {
    "@iktakahiro/markdown-it-katex": "^3.1.0",
    "@nuxtjs/axios": "^5.6.0",
    "@nuxtjs/bulma": "^1.2.4",
    "@nuxtjs/feed": "^1.1.0",
    "@nuxtjs/markdownit": "^1.2.6",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "bulma": "^0.7.5",
    "markdown-it-highlightjs": "^3.0.0",
    "nuxt": "^2.9.1",
    "processmd": "^4.4.2",
    "vue-scrollto": "^2.15.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.1.2",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-standard": ">=14.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-node": ">=9.1.0",
    "eslint-plugin-nuxt": ">=0.4.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": ">=4.2.1",
    "eslint-plugin-standard": ">=4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "sass-loader": "^7.2.0",
    "vue-eslint-parser": "^6.0.4"
  }
}
