{
  "name": "just-a-note",
  "version": "1.0.0",
  "description": "A collection of notes and logs written by shnarazk",
  "author": "Shuji Narazaki",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "update": "processmd article/**/*.md --stdout --outputDir article/.json > article/.json/db.json",
    "now": "npm run update && npm run generate && nuxt start"
  },
  "dependencies": {
    "@iktakahiro/markdown-it-katex": "^4.0.0",
    "@nuxtjs/axios": "^5.12.1",
    "@nuxtjs/bulma": "^1.3.0",
    "@nuxtjs/feed": "^2.0.0",
    "@nuxtjs/markdownit": "^1.2.10",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "bulma": "^0.9.0",
    "markdown-it-highlightjs": "^3.2.0",
    "nuxt": "^2.14.1",
    "processmd": "^4.5.0",
    "vue-scrollto": "^2.18.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": ">=14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-node": ">=11.1.0",
    "eslint-plugin-nuxt": ">=1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": ">=4.2.1",
    "eslint-plugin-standard": ">=4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "sass-loader": "^9.0.3",
    "vue-eslint-parser": "^7.1.0"
  }
}
